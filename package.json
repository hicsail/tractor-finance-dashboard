{
  "name": "tractor-finance-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prettier": "prettier -l \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:fix": "prettier -wl \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "dev": "vite",
    "start:dev": "npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "graphql": "graphql-faker graphql-faker/schema.graphql",
    "introspection": "graphql-codegen --config graphql-codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.117",
    "@mui/material": "^5.11.5",
    "@reduxjs/toolkit": "^1.9.1",
    "formik": "^2.2.9",
    "graphql": "^14.7.0",
    "mapbox-gl": "^2.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.21",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "recharts": "^2.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "graphql-faker": "^2.0.0-rc.25",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
